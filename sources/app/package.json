{
	"name": "cubyn-test",
	"version": "1.0.0",
	"description": "CRUD for parcels",
	"main": "index.js",
	"scripts": {
		"compile": "./node_modules/.bin/tsc",
		"test": "./node_modules/mocha/bin/mocha --reporter spec --require ts-node/register tests/**/*.spec.ts",
		"test:coverage": "nyc -e '.ts' --include-all-sources --r html -r lcov -r text npm run test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/playonlinux5683/cubyn-test.git"
	},
	"keywords": [],
	"author": "Sa√Ød LEBBIHI",
	"license": "ISC",
	"nyc": {
		"include": [
			"src/*.ts",
			"src/**/*.ts"
		],
		"exclude": [
			"typings"
		],
		"extension": [
			".ts"
		],
		"require": [
			"ts-node/register"
		],
		"reporter": [
			"json",
			"html"
		],
		"all": true
	},
	"bugs": {
		"url": "https://github.com/playonlinux5683/cubyn-test/issues"
	},
	"homepage": "https://github.com/playonlinux5683/cubyn-test#readme",
	"devDependencies": {
		"@types/chai": "^4.0.10",
		"@types/chai-http": "^3.0.5",
		"@types/cors": "^2.8.4",
		"@types/express": "^4.11.0",
		"@types/lodash": "^4.14.117",
		"@types/mocha": "^5.2.5",
		"@types/mongoose": "^5.2.19",
		"@types/morgan": "^1.7.35",
		"@types/node": "^10.12.0",
		"@types/sinon": "^5.0.5",
		"chai": "^4.1.2",
		"chai-http": "^4.2.0",
		"mocha": "^5.2.0",
		"nyc": "^13.1.0",
		"sinon": "^7.0.0",
		"sinon-mongoose": "^2.2.1",
		"ts-node": "^7.0.1",
		"typescript": "^3.1.3"
	},
	"dependencies": {
		"body-parser": "^1.18.2",
		"config": "^2.0.1",
		"cors": "^2.8.4",
		"express": "^4.16.2",
		"lodash": "^4.17.4",
		"mongoose": "^5.3.4",
		"morgan": "^1.9.0"
	}
}
